<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="Text.tabs" parent="Theme.AppCompat.NoActionBar">
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">20sp</item>
    </style>


    <style name="EditText.edit" parent="Theme.AppCompat.NoActionBar">
        <item name="android:textColor">#003233</item>
        <item name="android:textColorHint">#30003233</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">17sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:background">@drawable/bg_edit_text</item>

    </style>

    <style name="EditText.edit.field.white" parent="Theme.AppCompat.NoActionBar">
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">#9B9B9B</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">17sp</item>
        <item name="android:maxLines">1</item>
<!--        <item name="android:background">@drawable/bg_rounded_edit_text</item>-->

    </style>


    <style name="myMaterialButton003233" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="backgroundTint">#003233</item>
    </style>

    <style name="myMaterialButtonwhite" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="backgroundTint">#FFFFFF</item>
    </style>

    <style name="RoundedImageView.3dp" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">3dp</item>
    </style>

    <style name="RoundedImageView.5p" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">5dp</item>
    </style>

    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources>